<odoo>
    <record id="view_export_wizard_form" model="ir.ui.view">
        <field name="name">export.wizard.form</field>
        <field name="model">export.wizard</field>
        <field name="arch" type="xml">
            <form string="Export Invoices">
                <group>
                    <field name="start_date" />
                    <field name="end_date" />
                </group>
                <footer>
                    <button string="Export" type="object" name="action_export" class="btn-primary" />
                    <button string="Close" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>